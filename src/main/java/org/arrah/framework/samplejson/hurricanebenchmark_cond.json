{
  "sparkconfig": 
    {
      "appname": "TransformRunner",
      "master":"local[3]",
      "verbose":"true",
      "noexecutor":"3",
      "nocore":"3",
      "executormemory":"3G",
      "confparam":"spark.speculation,false,spark.hadoop.mapreduce.map.speculative,false,spark.hadoop.mapreduce.reduce.speculative,false"
    },
  "datasources": [
    {
      "name": "SampleDataJDBC",
      "format": "jdbc",
      "jdbcparam":"url,jdbc:mysql://localhost:3306/mysql,driver,com.mysql.jdbc.Driver,user,root,password,root,dbtable,(select * from help_category) AS T,partitionColumn,parent_category_id,lowerBound,0,upperBound,10000,numPartitions,10",
      "selectedColumns": []
    },
    {
      "name": "SampleData",
      "location":"/Users/vsingh007c/Downloads/PPV_sample_data/spark_MATERIAL/MATERIAL.txt",
      "locationType": "static",
      "format": "|",
      "selectedColumns": []
    }
  ],
  "transformations": [
    
    {
      "name": "ConditionData",
      "type": "enrichment",
      "source": "SampleDataJDBC",
      "priority": 1,
      "cache": false,
      "conditions": [
        {
          "condition": "conditionalcolumn",
          "aggrcondition": "condcol: IF (help_category_id > parent_category_id) THEN parent_category_id * parent_category_id ELSEIF (help_category_id < 5) THEN 'fiveValue' OTHERWISE 'default' "
        }
      ]
    },
    {
    "name": "selectExprData",
      "type": "filter",
      "source": "ConditionData",
      "priority": 2,
      "cache": false,
      "conditions": [
        {
          "condition": "selectexpr",
          "value": ["file","selectexprfile.txt"],
          "datatype": "String"
        }
      ]
    }
   
  ],
  "outputs": [
    {
      "name": "BASE_TABLE",
      "location": "/Users/vsingh007c/Downloads/",
      "sources": ["UDFData-Filter-newCol"],
      "format": "parquet",
      "savemode": "append",
      "selectedColumns": []
    }
  ]
}